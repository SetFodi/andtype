{
  "name": "andtype",
  "version": "1.0.0",
  "description": "A beautiful typing test application inspired by MonkeyType",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder --win --x64",
    "pack": "electron-builder --dir"
  },
  "author": "AndType",
  "license": "MIT",
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "electron-packager": "^17.1.2"
  },
  "build": {
    "appId": "com.andtype.app",
    "productName": "AndType",
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": [
        {
          "target": "dir",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "assets/icon.ico"
    },
    "forceCodeSigning": false
  }
}
